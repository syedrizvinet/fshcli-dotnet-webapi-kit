{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "PersistenceConfiguration": {
    "UseMsSql": false,
    "UsePostgres": true,
    "connectionStrings": {
      "mssql": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=dnwebapi;Integrated Security=True;MultipleActiveResultSets=True",
      "postgres": "Host=localhost;Database=dnwebapi;Username=postgres;Password=root;Include Error Detail=true"
    }
  }
}